{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "displayName": true
      }
    ],
    "@babel/plugin-syntax-class-properties",
    "@babel/plugin-syntax-import-meta"
  ]
}
